pipeline {
    agent any

    stages {
        stage('podman') {
            steps {
                sudo git clone https://github.com/JunPyo0117/CI-CD.git
                sudo podman build -t tomcat:test -f /home/ubuntu/CI-CD/tomcatDockerfile .
		sudo podman tag tomcat:test 52.79.48.121:30002/ajp-repository/tomcat:harbor
		sudo podman push 52.79.48.121:30002/ajp-repository/tomcat-repository/tomcat:harbor
            }
        }
    }
}
